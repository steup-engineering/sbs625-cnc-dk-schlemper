<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>
  <vbox>
    <labelframe text="Säge">
      <label text="Motordrehzahl UPM"/>
      <bar>
        <halpin>"spindle-rpm"</halpin>
        <min_>0.0</min_>
        <max_>6000.0</max_>
        <bgcolor>"lightgrey"</bgcolor>
        <fillcolor>"green"</fillcolor>
        <height>14</height>
        <width>125</width>
      </bar>
      <label text="Motorstrom %"/>
      <bar>
        <halpin>"spindle-torque"</halpin>
        <min_>0.0</min_>
        <max_>100.0</max_>
        <bgcolor>"lightgrey"</bgcolor>
        <fillcolor>"red"</fillcolor>
        <height>14</height>
        <width>125</width>
        <range1>0.0,70.0,"green"</range1>
        <range2>70.0,90.0,"yellow"</range2>
        <range3>90.0,100.0,"red"</range3>
      </bar>
      <checkbutton>
        <halpin>"water-disable"</halpin>
        <text>"Wasser aus"</text>
      </checkbutton>
    </labelframe>
    <labelframe text="Tisch">
      <table>
        <tablerow/>
          <label text="Kipparme:"/>
          <button>
            <width>2</width>
            <halpin>"table-arms-on"</halpin>
            <text>"auf"</text>
          </button>
           <led>
             <halpin>"table-arms-is-on"</halpin>
             <size>20</size>
             <on_color>"red"</on_color>
             <off_color>"gray"</off_color>
          </led>
          <button>
            <width>2</width>
            <halpin>"table-arms-off"</halpin>
            <text>"ab"</text>
          </button>
        <tablerow/>
          <label text="Haltemagnet:"/>
          <button>
            <width>2</width>
            <halpin>"table-magnet-on"</halpin>
            <text>"ein"</text>
          </button>
           <led>
             <halpin>"table-magnet-is-on"</halpin>
             <size>20</size>
             <on_color>"red"</on_color>
             <off_color>"gray"</off_color>
          </led>
          <button>
            <width>2</width>
            <halpin>"table-magnet-off"</halpin>
            <text>"aus"</text>
          </button>
        <tablerow/>
          <label text="Raste:"/>
          <button>
            <width>2</width>
            <halpin>"table-lock-on"</halpin>
            <text>"ein"</text>
          </button>
           <led>
             <halpin>"table-lock-is-on"</halpin>
             <size>20</size>
             <on_color>"red"</on_color>
             <off_color>"gray"</off_color>
          </led>
          <button>
            <width>2</width>
            <halpin>"table-lock-off"</halpin>
            <text>"aus"</text>
          </button>
      </table>
    </labelframe>
    <labelframe text="Manuelle Steuerung">
      <hbox>
        <button>
          <width>4</width>
          <halpin>"home-z"</halpin>
          <text>"Home Z"</text>
        </button>
        <button>
          <width>4</width>
          <halpin>"home-x"</halpin>
          <text>"Home X"</text>
        </button>
        <button>
          <width>5</width>
          <halpin>"home-xy"</halpin>
          <text>"Home X/Y"</text>
        </button>
      </hbox>

      <hbox>
        <button>
          <width>2</width>
          <halpin>"manu-c-0"</halpin>
          <text>"C0"</text>
        </button>
        <button>
          <width>2</width>
          <halpin>"manu-c-90"</halpin>
          <text>"C90"</text>
        </button>
        <button>
          <width>2</width>
          <halpin>"manu-c-180"</halpin>
          <text>"C180"</text>
        </button>
        <button>
          <width>2</width>
          <halpin>"manu-c-270"</halpin>
          <text>"C270"</text>
        </button>
      </hbox>

      <hbox>
        <button>
          <width>2</width>
          <halpin>"manu-a-0"</halpin>
          <text>"A0"</text>
        </button>
        <button>
          <width>2</width>
          <halpin>"manu-a-30"</halpin>
          <text>"A30"</text>
        </button>
        <button>
          <width>2</width>
          <halpin>"manu-a-45"</halpin>
          <text>"A45"</text>
        </button>
        <button>
          <width>2</width>
          <halpin>"manu-a-90"</halpin>
          <text>"A90"</text>
        </button>
      </hbox>
      <hbox>
        <label text="messen:"/>
        <button>
          <width>4</width>
          <halpin>"toolprobe-blade"</halpin>
          <text>"Säge"</text>
        </button>
        <button>
          <width>4</width>
          <halpin>"toolprobe-mill"</halpin>
          <text>"Fräser"</text>
        </button>
      </hbox>
      <hbox>
        <button>
          <width>4</width>
          <halpin>"mancut-left"</halpin>
          <text><![CDATA["<<--"]]></text>
        </button>
        <button>
          <width>4</width>
          <halpin>"mancut-stop"</halpin>
          <text>"STOP"</text>
        </button>
        <button>
          <width>4</width>
          <halpin>"mancut-right"</halpin>
          <text><![CDATA["-->>"]]></text>
        </button>
      </hbox>
    </labelframe>

    <labelframe text="Sauger">
      <hbox>
        <button>
          <width>6</width>
          <halpin>"vac-manu-pickup"</halpin>
          <text>"aufnehmen"</text>
        </button>
        <button>
          <width>6</width>
          <halpin>"vac-manu-release"</halpin>
          <text>"ablegen"</text>
        </button>
      </hbox>
      <table>
        <tablerow/>
          <checkbutton>
            <halpin>"vac-manu-0l"</halpin>
            <text>"1L"</text>
          </checkbutton>
          <label text="1L"/>
          <label text=" # "/>
          <label text="1R"/>
          <checkbutton>
            <halpin>"vac-manu-0r"</halpin>
            <text>"1R"</text>
          </checkbutton>
        <tablerow/>
          <label text="1L"/>
          <label text="1L"/>
          <label text=" # "/>
          <label text="2R"/>
          <checkbutton>
            <halpin>"vac-manu-1r"</halpin>
            <text>"2R"</text>
          </checkbutton>
        <tablerow/>
          <checkbutton>
            <halpin>"vac-manu-1l"</halpin>
            <text>"2L"</text>
          </checkbutton>
          <checkbutton>
            <halpin>"vac-manu-2l"</halpin>
            <text>"3L"</text>
          </checkbutton>
          <label text=" # "/>
          <checkbutton>
            <halpin>"vac-manu-2r"</halpin>
            <text>"3R"</text>
          </checkbutton>
          <checkbutton>
            <halpin>"vac-manu-3r"</halpin>
            <text>"4R"</text>
          </checkbutton>
      </table>
    </labelframe>

    <labelframe text="Laser">
      <table>
        <tablerow/>
          <label text="Kreutz:"/>
          <vbox>
            <hbox>
              <button>
                <width>1</width>
                <halpin>"laser-cross-on"</halpin>
                <text>"ein"</text>
              </button>
               <led>
                 <halpin>"laser-cross-is-on"</halpin>
                 <size>20</size>
                 <on_color>"red"</on_color>
                 <off_color>"gray"</off_color>
              </led>
              <button>
                <width>1</width>
                <halpin>"laser-cross-off"</halpin>
                <text>"aus"</text>
              </button>
              <button>
                <width>3</width>
                <halpin>"laser-cross-show"</halpin>
                <text>"zeige"</text>
              </button>
            </hbox>
            <hbox>
              <button>
                <width>2</width>
                <halpin>"laser-cross-touch-x"</halpin>
                <text>"X=0"</text>
              </button>
              <button>
                <width>2</width>
                <halpin>"laser-cross-touch-y"</halpin>
                <text>"Y=0"</text>
              </button>
              <button>
                <width>2</width>
                <halpin>"laser-cross-matprobe"</halpin>
                <text>"Mat"</text>
              </button>
            </hbox>
          </vbox>
      </table>
      <table>
        <tablerow/>
          <label text="Strich:"/>
          <vbox>
            <hbox>
              <button>
                <width>1</width>
                <halpin>"laser-line-on"</halpin>
                <text>"ein"</text>
              </button>
               <led>
                 <halpin>"laser-line-is-on"</halpin>
                 <size>20</size>
                 <on_color>"red"</on_color>
                 <off_color>"gray"</off_color>
              </led>
              <button>
                <width>1</width>
                <halpin>"laser-line-off"</halpin>
                <text>"aus"</text>
              </button>
            </hbox>
            <hbox>
              <button>
                <width>2</width>
                <halpin>"laser-line-touch-x"</halpin>
                <text>"X=0"</text>
              </button>
              <button>
                <width>2</width>
                <halpin>"laser-line-touch-y"</halpin>
                <text>"Y=0"</text>
              </button>
              <button>
                <width>4</width>
                <halpin>"laser-line-touch-y-table-x"</halpin>
                <text>"Y=0/X=T"</text>
              </button>
            </hbox>
          </vbox>
      </table>
      <table>
        <tablerow/>
          <label text="Tisch:"/>
          <vbox>
            <hbox>
              <button>
                <width>2</width>
                <halpin>"table-touch-x"</halpin>
                <text>"X=0"</text>
              </button>
              <button>
                <width>2</width>
                <halpin>"table-touch-y"</halpin>
                <text>"Y=0"</text>
              </button>
            </hbox>
          </vbox>
      </table>
    </labelframe>
    <labelframe text="Handrad-Sonderfunktion">
      <table>
        <tablerow/>
          <button>
            <width>6</width>
            <halpin>"pot-spindle"</halpin>
            <text>"Spindel"</text>
          </button>
          <led>
             <halpin>"pot-is-spindle"</halpin>
             <size>20</size>
             <on_color>"red"</on_color>
             <off_color>"gray"</off_color>
          </led>
          <button>
            <width>6</width>
            <halpin>"pot-vel-max"</halpin>
            <text>"V max"</text>
          </button>
          <led>
             <halpin>"pot-is-vel-max"</halpin>
             <size>20</size>
             <on_color>"red"</on_color>
             <off_color>"gray"</off_color>
          </led>
      </table>
    </labelframe>
  </vbox>
</pyvcp>
